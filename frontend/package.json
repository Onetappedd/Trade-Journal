    "format:check": "prettier --check .",
    "scan:weird": "node scripts/scan-weird-chars.mjs",
    "check-env": "node scripts/check-env.js",
    "check:routes": "node -e \"console.log('market routes present')\"",
    "check:all": "npm run scan:weird && npm run format:check && npm run type-check && npm run lint && npm run check:routes && npm run check-env",
    "check-db": "node scripts/check-db.js",
    "check-tables": "node scripts/check-table-structure.js",
    "test-real-data": "node scripts/test-real-data.js",
    "test-polygon": "node scripts/test-polygon.js",
